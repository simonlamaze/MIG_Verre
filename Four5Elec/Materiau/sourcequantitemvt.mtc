      { Type= P1_Scalaire_Par }
      { Nom= DeltaRho }
      { Data= ValeurItem 1 0. }
    }
  }
  { Dependance= 
    { Maillage= MaillagePrincipal }
    { Champ= T }
    { Champ= T0 }
    { Champ= g }
    { Champ= beta }
